{
  "dashboard": {
    "title": "IdeaMine Orchestrator - Overview",
    "uid": "ideamine-orchestrator-overview",
    "timezone": "browser",
    "schemaVersion": 30,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "Active Runs",
        "type": "stat",
        "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
        "targets": [
          {
            "expr": "count(ideamine_run_status{status='running'})",
            "legendFormat": "Active Runs"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "orientation": "auto"
        }
      },
      {
        "id": 2,
        "title": "Completed Runs (24h)",
        "type": "stat",
        "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 },
        "targets": [
          {
            "expr": "sum(increase(ideamine_run_completed_total[24h]))",
            "legendFormat": "Completed"
          }
        ]
      },
      {
        "id": 3,
        "title": "Success Rate",
        "type": "gauge",
        "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 },
        "targets": [
          {
            "expr": "sum(rate(ideamine_run_completed_total{status='success'}[5m])) / sum(rate(ideamine_run_completed_total[5m])) * 100",
            "legendFormat": "Success %"
          }
        ],
        "options": {
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                { "value": 0, "color": "red" },
                { "value": 90, "color": "yellow" },
                { "value": 95, "color": "green" }
              ]
            },
            "unit": "percent"
          }
        }
      },
      {
        "id": 4,
        "title": "Total Cost (24h)",
        "type": "stat",
        "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 },
        "targets": [
          {
            "expr": "sum(increase(ideamine_run_cost_usd_total[24h]))",
            "legendFormat": "Cost"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "decimals": 2
          }
        }
      },
      {
        "id": 5,
        "title": "Run Duration Distribution",
        "type": "heatmap",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
        "targets": [
          {
            "expr": "rate(ideamine_run_duration_seconds_bucket[5m])",
            "format": "heatmap",
            "legendFormat": "{{le}}"
          }
        ],
        "options": {
          "calculate": false,
          "calculation": {},
          "cellGap": 2,
          "color": {
            "exponent": 0.5,
            "fill": "dark-orange",
            "mode": "scheme",
            "scale": "exponential",
            "scheme": "Spectral",
            "steps": 128
          },
          "exemplars": {
            "color": "rgba(255,0,255,0.7)"
          },
          "filterValues": {
            "le": 1e-9
          },
          "legend": {
            "show": true
          },
          "rowsFrame": {
            "layout": "auto"
          },
          "tooltip": {
            "show": true,
            "yHistogram": false
          },
          "yAxis": {
            "axisPlacement": "left",
            "reverse": false,
            "unit": "s"
          }
        }
      },
      {
        "id": 6,
        "title": "Phase Completion Rate",
        "type": "timeseries",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
        "targets": [
          {
            "expr": "rate(ideamine_phase_completed_total{status='success'}[5m])",
            "legendFormat": "{{phase}}"
          }
        ],
        "options": {
          "tooltip": {
            "mode": "multi"
          },
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "calcs": ["mean", "lastNotNull"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10
            },
            "unit": "ops"
          }
        }
      },
      {
        "id": 7,
        "title": "Token Usage by Phase",
        "type": "barchart",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
        "targets": [
          {
            "expr": "sum by (phase) (increase(ideamine_phase_tokens_used_total[24h]))",
            "legendFormat": "{{phase}}"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "xTickLabelRotation": 0,
          "xTickLabelSpacing": 100
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "bars",
              "fillOpacity": 80,
              "gradientMode": "none",
              "hideFrom": {
                "tooltip": false,
                "viz": false,
                "legend": false
              },
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "none"
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            }
          }
        }
      },
      {
        "id": 8,
        "title": "Worker Pool Utilization",
        "type": "timeseries",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
        "targets": [
          {
            "expr": "ideamine_worker_busy_count / ideamine_worker_total_count * 100",
            "legendFormat": "Utilization %"
          },
          {
            "expr": "ideamine_worker_total_count",
            "legendFormat": "Total Workers"
          }
        ],
        "fieldConfig": {
          "overrides": [
            {
              "matcher": { "id": "byName", "options": "Utilization %" },
              "properties": [
                { "id": "custom.axisPlacement", "value": "left" },
                { "id": "unit", "value": "percent" },
                { "id": "max", "value": 100 }
              ]
            },
            {
              "matcher": { "id": "byName", "options": "Total Workers" },
              "properties": [
                { "id": "custom.axisPlacement", "value": "right" },
                { "id": "unit", "value": "short" }
              ]
            }
          ]
        }
      },
      {
        "id": 9,
        "title": "Gate Pass Rate by Phase",
        "type": "bargauge",
        "gridPos": { "h": 6, "w": 8, "x": 0, "y": 20 },
        "targets": [
          {
            "expr": "sum by (phase) (rate(ideamine_gate_passed_total[24h])) / sum by (phase) (rate(ideamine_gate_evaluated_total[24h])) * 100",
            "legendFormat": "{{phase}}"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient",
          "showUnfilled": true
        },
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "unit": "percent",
            "thresholds": {
              "steps": [
                { "value": 0, "color": "red" },
                { "value": 70, "color": "yellow" },
                { "value": 90, "color": "green" }
              ]
            }
          }
        }
      },
      {
        "id": 10,
        "title": "Idempotent Cache Hit Rate",
        "type": "stat",
        "gridPos": { "h": 6, "w": 8, "x": 8, "y": 20 },
        "targets": [
          {
            "expr": "sum(rate(ideamine_idempotency_cache_hits_total[5m])) / sum(rate(ideamine_idempotency_checks_total[5m])) * 100",
            "legendFormat": "Hit Rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 1,
            "thresholds": {
              "steps": [
                { "value": 0, "color": "red" },
                { "value": 10, "color": "yellow" },
                { "value": 30, "color": "green" }
              ]
            }
          }
        },
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        }
      },
      {
        "id": 11,
        "title": "Average Run Duration",
        "type": "timeseries",
        "gridPos": { "h": 6, "w": 8, "x": 16, "y": 20 },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(ideamine_run_duration_seconds_bucket[5m]))",
            "legendFormat": "P50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(ideamine_run_duration_seconds_bucket[5m]))",
            "legendFormat": "P95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(ideamine_run_duration_seconds_bucket[5m]))",
            "legendFormat": "P99"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth"
            }
          }
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "datasource",
          "type": "datasource",
          "query": "prometheus"
        },
        {
          "name": "phase",
          "type": "query",
          "datasource": "${datasource}",
          "query": "label_values(ideamine_phase_completed_total, phase)",
          "multi": true,
          "includeAll": true
        }
      ]
    },
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "annotations": {
      "list": [
        {
          "datasource": "${datasource}",
          "name": "Deployments",
          "expr": "ideamine_deployment_event",
          "tagKeys": "version,environment"
        }
      ]
    }
  }
}
